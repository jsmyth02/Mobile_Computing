                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight=".50"
                            android:textStyle="bold"
                            android:text="Team One"/>

                        <TextView
                            android:id="@+id/Home_Team_Date_One_Played"
                            android:layout_width="wrap_content"
                            android:layout_weight=".25"
                            android:layout_height="wrap_content"/>

